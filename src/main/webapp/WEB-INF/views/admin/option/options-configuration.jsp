<div class="option-configuration">
    <h4>Option Configuration</h4>

    <div>
        <label for="bursaPercent">Profit Percent</label>
        <input type="text" name="bursaPercent" value="${option.optionConfiguration.profitPercent}"/>
    </div>
    <div>
        <label for="lossInsurance">Loss insurance</label>
        <input type="text" name="lossInsurance" value="${option.optionConfiguration.lossInsurance}"/>
    </div>
    <div>
        <label for="maxLimitation">Max limitation</label>
        <input type="text" name="maxLimitation" value="${option.optionConfiguration.maxLimitation}"/>
    </div>
    <div>
        <label for="minLimitation">Min limitation</label>
        <input type="text" name="minLimitation" value="${option.optionConfiguration.minLimitation}"/>
    </div>
</div>